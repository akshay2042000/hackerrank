#include <stdio.h>
int main()
{
  int n;
  scanf("%d",&n);
  int x;
  x=n%2000;
  printf("%d ",n/2000);
 
  printf("%d ",x/500);
   x=x%500;
  
  printf("%d ",x/100);
  x=x%100;

  printf("%d ",x/50);
    x=x%50;

  printf("%d ",x/20);
    x=x%20;

  printf("%d ",x/10);
    x=x%10;
  
  printf("%d ",x/5);
  x=x%5;

  printf("%d ",x/2);
    x=x%2;
 
  printf("%d ",x/1);
   x=x%1;
  
    return 0;
}
